<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Read-only int property</Title>
      <Shortcut>ri</Shortcut>
      <Description>Read-only int property</Description>
      <Author>Benjamin Day</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>ClassName</ID>
          <ToolTip></ToolTip>
          <Default>ClassName</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[private int _$ClassName$;

public int $ClassName$
{
    get
    {
         return _$ClassName$;$end$
    }
}]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>