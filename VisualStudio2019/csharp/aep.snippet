<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Assert.AreEqual on two properties</Title>
      <Shortcut>aep</Shortcut>
      <Description>Assert.AreEqual on two properties</Description>
      <Author>Benjamin Day</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>datatype</ID>
          <ToolTip></ToolTip>
          <Default>datatype</Default>
        </Literal>
        <Literal>
          <ID>propertyName</ID>
          <ToolTip></ToolTip>
          <Default>propertyName</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[Assert.AreEqual<$datatype$>(expected.$propertyName$, actual.$propertyName$, "$propertyName$");$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>