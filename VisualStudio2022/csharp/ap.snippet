<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Auto-implemented property</Title>
      <Shortcut>ap</Shortcut>
      <Description>Auto-implemented property</Description>
      <Author>Benjamin Day</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Type</ID>
          <ToolTip></ToolTip>
          <Default>Type</Default>
        </Literal>
        <Literal>
          <ID>PropertyName</ID>
          <ToolTip></ToolTip>
          <Default>PropertyName</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[public $Type$ $PropertyName$ { get; set; } = default();$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>